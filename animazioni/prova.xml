<?xml version="1.0" encoding="utf-8"?>
<storia>
        <cing1>
                <segmento>2</segmento>
                <messaggio>Growl</messaggio>
                <messaggio>growl...</messaggio> 
        </cing1>
        <interlocutore>
                <segmento>2</segmento>
                <messaggio>Esci muoviti! Il califfo sta parlando e manchi solo tu!</messaggio>
                <messaggio>Se non vai sarai messo al bando</messaggio>
                <messaggio>Tutti devono essere presenti!</messaggio>
                <messaggio>Sbrigati!</messaggio>
        </interlocutore>
        <interlocutore1>
                <segmento>1</segmento>
                <messaggio>uno di uno</messaggio>
        </interlocutore1>
         <villager>
                <segmento>1</segmento>
                <messaggio>Seguimi!</messaggio>
        </villager>
        <aio>
                <segmento>1</segmento>
                <messaggio>Le tasse sono state alzate ancora!</messaggio>
                <messaggio>E' inutile che vi lamentiate</messaggio>
                <messaggio>Se non pagate morirete!</messaggio>
        </aio>
        <aio2>
                <segmento>1</segmento>
                <messaggio>la birra è gratis!</messaggio>
                <messaggio>prendila!</messaggio>
        </aio2>
        <msg1>
                <segmento>1</segmento>
                <messaggio>Questo discorso parte da un oggetto evento!</messaggio>
                <messaggio>Non c'è nessuna animazione</messaggio>
        </msg1>
        <msg2>
                <segmento>1</segmento>
                <messaggio>Il cane Wolf ti seguirà ovunque.</messaggio>
                <messaggio>se gli vai vicino per prenderlo con te.</messaggio>
                <messaggio>Nell'armeria puoi prendere armi </messaggio>
                <messaggio>da provare nel poligono di tiro...</messaggio>
        </msg2>
        <msg3>
                <segmento>1</segmento>
                <messaggio>Puoi tirare con l'arco agli animali selvatici</messaggio>
                <messaggio>oppure puoi prendere gli animali domestici</messaggio>
                <messaggio>usando il lasso (tasto L) </messaggio>
        </msg3>
         <msginiziale>
                <segmento>1</segmento>
                <messaggio>Sei un soldato che ha appena finito la guerra nel deserto</messaggio>
                <messaggio>il tuo villaggio e stato bruciato e raso al suolo.</messaggio>
                <messaggio>tua moglie e stata decapitata, i tuoi figli sono stati presi come schiavi.</messaggio>
                <messaggio>Siete soppravissuti solo in 20. Vi siete divisi. Sei arrivato in questo villaggio.</messaggio>
                <messaggio>Ti ritrovi in una casa decadente.</messaggio>
        </msginiziale>
</storia>